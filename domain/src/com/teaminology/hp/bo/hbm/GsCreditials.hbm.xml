<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.teaminology.hp.bo">

    <class name="GsCreditials" table="gs_creditials">
        <id name="gsCreditialsId" column="gs_creditials_id" type="java.lang.Integer">
            <generator class="native"/>
        </id>

        <property name="url" column="url" type="java.lang.String"/>
        <property name="wsdlUrl" column="wsdl_url" type="java.lang.String"/>
        <property name="companyId" column="company_id" type="java.lang.Integer"/>
        <property name="userName" column="user_name" type="java.lang.String" unique="true"/>
        <property name="password" column="password" type="java.lang.String"/>
        <property name="createTime" column="create_time" type="java.util.Date"/>
        <property name="createdBy" column="created_by" type="java.lang.Integer"/>
        <property name="updatedBy" column="updated_by" type="java.lang.Integer"/>
        <property name="updateDate" column="update_date" type="java.util.Date"/>
        <property name="isActive" column="is_active" type="java.lang.String"/>

    </class>
</hibernate-mapping>
