<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.teaminology.hp.bo">

    <class name="UserLanguages" table="user_languages">
        <id name="userLangId" column="user_language_id" type="java.lang.Integer">
            <generator class="native"/>
        </id>

        <property name="userId" column="user_id" type="java.lang.Integer"/>
        <!--  <property name="langId" column="lang_id" type="java.lang.String" />  -->
        <property name="createdBy" column="created_by" type="java.lang.Integer"/>
        <property name="createDate" column="create_date" type="java.util.Date"/>
        <property name="updatedBy" column="updated_by" type="java.lang.Integer"/>
        <property name="updateDate" column="update_date" type="java.util.Date"/>
        <property name="isActive" column="is_active" type="java.lang.String"/>

        <many-to-one
                class="com.teaminology.hp.bo.lookup.Language"
                column="language_id"
                name="languages"
                not-null="false"
                lazy="false"
                cascade="all"/>


    </class>
</hibernate-mapping>