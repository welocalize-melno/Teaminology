<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.teaminology.hp.bo">

    <class name="RolePrivileges" table="role_privileges">
        <id name="rolePrivilegesId" column="role_privileges_id" type="java.lang.Integer">
            <generator class="native"/>
        </id>

        <property name="createDate" column="create_date" type="java.util.Date"/>
        <property name="createdBy" column="created_by" type="java.lang.Integer"/>
        <property name="updatedBy" column="updated_by" type="java.lang.Integer"/>
        <property name="updateDate" column="update_date" type="java.util.Date"/>
        <property name="isActive" column="is_active" type="java.lang.String"/>
        <many-to-one class="com.teaminology.hp.bo.Role"
                     column="role_id" lazy="false" name="role" not-null="false"/>
        <many-to-one
                class="com.teaminology.hp.bo.Privileges"
                column="privilege_id"
                name="privileges"
                not-null="false"
                lazy="false"
                cascade="all"/>

    </class>
</hibernate-mapping>
