<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.teaminology.hp.bo">

    <class name="Company" table="company_lookup">
        <id name="companyId" column="company_id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="companyName" column="company_name" type="java.lang.String"/>
        <property name="poc" column="poc" type="java.lang.String"/>
        <property name="emailId" column="email_id" type="java.lang.String"/>
        <property name="createdBy" column="created_by" type="java.lang.Integer"/>
        <property name="createDate" column="create_date" type="java.util.Date"/>
        <property name="updatedBy" column="updated_by" type="java.lang.Integer"/>
        <property name="updateDate" column="update_date" type="java.util.Date"/>
        <property name="isActive" column="is_active" type="java.lang.String"/>
        <property name="contextRoot" column="context_root" type="java.lang.String"/>
        <property name="url" column="url" type="java.lang.String"/>
        <property name="userName" column="user_name" type="java.lang.String"/>
        <property name="password" column="password" type="java.lang.String"/>
    </class>
</hibernate-mapping>
